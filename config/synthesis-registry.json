{
  "modules": [
    {
      "id": "botanical-name",
      "path": "../synthesis/process-botanical-name.js",
      "enabled": true,
      "description": "Validates botanical names and returns current nomenclature status"
    },
    {
      "id": "native-checker",
      "path": "../synthesis/process-native-checker.js",
      "enabled": false,
      "description": "Determines if plant species is native to Southeast Michigan"
    },
    {
      "id": "external-reference-urls",
      "path": "../synthesis/process-external-reference-urls.js",
      "enabled": true,
      "description": "Discovers URLs for plant species across botanical reference websites"
    },
    {
      "id": "common-names",
      "path": "../synthesis/process-common-names.js",
      "enabled": true,
      "description": "Identifies all common/vernacular names used in Southeast Michigan and adjacent regions"
    },
    {
      "id": "previous-botanical",
      "path": "../synthesis/process-previous-botanical.js",
      "enabled": true,
      "description": "Retrieves botanical synonyms (legacy binomial names) from GBIF for cross-reference purposes"
    },
    {
      "id": "michigan-flora",
      "path": "../synthesis/process-michigan-flora.js",
      "enabled": true,
      "description": "Retrieves ecological metrics from Michigan Flora dataset: C value, wetness, growth form, and life cycle"
    },
    {
      "id": "inaturalist-observations",
      "path": "../synthesis/process-inaturalist.js",
      "enabled": true,
      "description": "Appends Wikipedia URL to references, adds SE Michigan observation histogram and Wikipedia summary from iNaturalist"
    },
    {
      "id": "bonap-range-map",
      "path": "../synthesis/process-bonap.js",
      "enabled": true,
      "description": "Discovers BONAP county-level range map image URL"
    },
    {
      "id": "lakecounty-cache",
      "path": "../synthesis/process-lakecounty-cache.js",
      "enabled": true,
      "description": "Syncs Google Drive parsed PDF files to local cache for Lake County data"
    },
    {
      "id": "lakecounty-images",
      "path": "../synthesis/process-lakecounty-images.js",
      "enabled": true,
      "description": "Extracts image filenames from Lake County parsed PDF JSON files"
    },
    {
      "id": "similar-species",
      "path": "../synthesis/process-similar-species.js",
      "enabled": true,
      "description": "Identifies similar species and distinguishing features using tiered source-backed and inferred data"
    },
    {
      "id": "3tier-collection_mature_seed_color",
      "path": "../synthesis/process-3tier-field.js",
      "enabled": true,
      "description": "3-tier LLM prompting for Seed Color at Maturity field",
      "exportName": "seedColorModule"
    },
    {
      "id": "3tier-collection_miss_risk",
      "path": "../synthesis/process-3tier-field.js",
      "enabled": true,
      "description": "3-tier LLM prompting for Collection Miss Risk field",
      "exportName": "missRiskModule"
    },
    {
      "id": "3tier-processing_hazards",
